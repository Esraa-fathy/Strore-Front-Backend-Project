{
    "spec_dir": "build",
    "spec_files": [
      "**/**/*_[sS]pec.?(m)js", 
      "build/Tests/*.js"
    ],
    "helpers": [
      "Tests/helper/*.?(m)js" 
    ],
    "reporters": [
      {
        "name": "jasmine-spec-reporter#SpecReporter",
        "options": {
          "displayStacktrace": "all"
        }
      }
    ],
    "stopSpecOnExpectationFailure": false,
    "random": false
  }